<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="imd.ufrn.br.purposesong.view.MainView">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="labelContent"/>
    <Label text="Aperte abaixo para adiconar um + na label acima"/>
    <Button text="Bem vindo a tela principal!" onAction="#onButtonClick"/>

    <Button text="Aperte aq para ir a tela de login" onAction="#goToLogin"/>
</VBox>
